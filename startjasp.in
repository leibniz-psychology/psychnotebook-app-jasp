#!/bin/bash

while getopts s: opt
do
   case $opt in
       s) SOCKET_PATH="$OPTARG";;
   esac
done

exec XPRA_PREFIX/bin/xpra start \
		--bind="$SOCKET_PATH" \
		--html=on \
		--daemon=no \
		--start='JASP_PREFIX/bin/jasp' \
		--av-sync=no \
		--speaker=off \
		--microphone=off \
		--server-idle-timeout=3600

